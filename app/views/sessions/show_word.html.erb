<%= include_gon %>

<table id="word-list-table" class="table table-striped table-hover ">
  <thead>
    <td>user</td>
    <td>tweet</td>
  </thead>
  <% @word_list.each do |t| %>
  <tr>
    <td><h4><%= t.user %></td>
    <td><h4><a href="<%= t.uri %>"><%= t.text %></a></td>
  </tr>
  <% end -%>
</table>
